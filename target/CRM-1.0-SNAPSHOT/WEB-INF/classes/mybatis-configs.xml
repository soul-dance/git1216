<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <!--日志功能 -->
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>

    <typeAliases>
        <package name="com.ry.settings.pojo"/>
        <package name="com.ry.workbench.pojo"/>
    </typeAliases>

    <!-- 加载映射文件 -->
    <mappers>
        <package name="com.ry.settings.dao"/>
        <package name="com.ry.workbench.dao"/>
    </mappers>

</configuration>